{
  "manifest_version": 2,
  "name": "MHTML Saver Test Suite",
  "version": "0.0.1",
  "description": "HanYang University DB LAB Crawler",
  "author": "Geunseong Jung",
  "homepage_url": "http://db.hayang.ac.kr",
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "popup/popup.html"
  },
  "options_page": "options/index.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "inject/main.ts"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": [
      "background/main.ts"
    ]
  },
  "permissions": [
    "<all_urls>",
    "pageCapture",
    "tabs",
    "storage",
    "webNavigation",
    "downloads",
    "downloads.open"
  ]
}